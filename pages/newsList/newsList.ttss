/* pages/newsList/newsList.ttss */
.news-list {
  position: relative;
  height: 90vh;
}

.pagination-container {
  position: fixed;
  bottom: 0;
  width: 100%;
  min-height: 52px;
  z-index: 1024;
  height: calc(52px + env(safe-area-inset-bottom) / 2);
  padding-bottom: calc(env(safe-area-inset-bottom) / 2);
  display: flex;
  justify-content: center;
}
